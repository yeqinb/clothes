{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "http://47.112.26.70:9010/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}